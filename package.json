{
  "name": "@enzedonline/quill-blot-formatter2",
  "version": "2.0.0",
  "description": "An update for quill-blot-formatter to make quilljs v2 compatible.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/enzedonline/quill-blot-formatter2",
  "bugs": {
    "url": "https://github.com/enzedonline/quill-blot-formatter2/issues"
  },
  "contributors": [
    {
      "name": "Richard Allen",
      "email": "enzedonline@users.noreply.github.com",
      "url": "https://github.com/enzedonline"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/enzedonline/quill-blot-formatter2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "quill",
    "blot",
    "image",
    "resize"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.7.0",
    "npm-run-all": "^4.1.2",
    "quill": "^2.0.2",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:ts && yarn build:webpack && yarn build:copy-ts-defs",
    "build:ts": "tsc",
    "build:copy-ts-defs": "copyfiles index.d.ts dist",
    "build:webpack": "webpack --mode production --progress",
    "dev:webpack": "yarn dev:delay && webpack --mode development --progress --watch",
    "dev:delay": "node -e \"setTimeout(() => {}, 3000)\"",
    "prepublishOnly": "yarn build"
  }
